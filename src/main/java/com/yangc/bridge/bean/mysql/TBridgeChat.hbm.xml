<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.yangc.bridge.bean.TBridgeChat" table="T_BRIDGE_CHAT">
		<comment>消息表</comment>
		<id name="id" type="java.lang.Long">
			<column name="ID" precision="22" scale="0" />
			<generator class="identity" />
		</id>
		<property name="uuid" type="java.lang.String">
			<column name="UUID" length="50">
				<comment>UUID</comment>
			</column>
		</property>
		<property name="from" type="java.lang.String">
			<column name="FROM" length="200">
				<comment>发送人</comment>
			</column>
		</property>
		<property name="to" type="java.lang.String">
			<column name="TO" length="200">
				<comment>接收人</comment>
			</column>
		</property>
		<property name="data" type="java.lang.String">
			<column name="DATA" length="1000">
				<comment>消息内容</comment>
			</column>
		</property>
		<property name="status" type="java.lang.Long">
			<column name="STATUS" precision="1" scale="0">
				<comment>消息状态:0未读;已读</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
